{
  "name": "gather",
  "version": "0.0.0",
  "description": "phone and letter territory information gatherer",
  "main": "index.js",
  "scripts": {
    "start": "node --inspect=5858 -r ts-node/register ./src/index.ts",
    "start:watch": "PORT=8989 nodemon",
    "test": "tape test/getAddress.spec.js",
    "build": "tsc"
  },
  "author": "@dillonchr",
  "license": "GPL-2.0",
  "dependencies": {
    "@dillonchr/domget": "^1.1.0",
    "@sentry/node": "5.7.0",
    "bluebird": "3.7.0",
    "puppeteer": "^1.15.0",
    "redis": "^2.8.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.30",
    "@types/node": "^13.9.1",
    "@types/puppeteer": "^2.0.1",
    "@types/redis": "^2.8.16",
    "tape": "^4.9.1",
    "ts-node": "^8.6.2",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.spec.ts",
      ".git",
      "**/*.spec.js"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  }
